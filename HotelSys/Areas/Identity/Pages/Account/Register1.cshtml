@page
@model RegisterModel
@{
    ViewData["Title"] = "Register";
    //Layout = "~/Views/Shared/_Layout2.cshtml";
}



<div class="page-wrapper">

    <!-- Page Content-->
    <div class="page-content-tab">

        <div class="container-fluid">
            <!-- Page-Title -->
            <div class="row">
                <div class="col-sm-12">
                    <div class="page-title-box">
                        <div class="float-end">
                            <ol class="breadcrumb">
                                <li class="breadcrumb-item"><a href="#">المشتركين </a></li>

                                <li class="breadcrumb-item active">حساب جديد </li>
                            </ol>
                        </div>
                        <h4 class="page-title">انشاء مستخدم جديد</h4>
                    </div>
                    <!--end page-title-box-->
                </div>
                <!--end col-->
            </div>

            <h6>..</h6>
            <hr />
            <div class="row">
                <div class="col-md-4">
                    <form asp-route-returnUrl="@Model.ReturnUrl" method="post">
                        <h4>انشاء حساب جديد</h4>
                        <hr />
                        <div asp-validation-summary="All" class="text-danger"></div>
                        <div class="form-group">
                            <label asp-for="Input.Email">البريد الالكتروني</label>
                            <input asp-for="Input.Email" class="form-control" />
                            <span asp-validation-for="Input.Email" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="Input.Password">كلمة السر </label>
                            <input asp-for="Input.Password" class="form-control" />
                            <span asp-validation-for="Input.Password" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="Input.ConfirmPassword">تاكيد كلمة السر </label>
                            <input asp-for="Input.ConfirmPassword" class="form-control" />
                            <span asp-validation-for="Input.ConfirmPassword" class="text-danger"></span>
                        </div>
                        <button type="submit" class="btn btn-primary">انشاء </button>
                    </form>
                </div>
            </div>




        </div>
    </div>
</div>



@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}


