            </th>
            <th>
                @Html.DisplayNameFor(model => model.PublicNot)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PrivateNote)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdSub)
