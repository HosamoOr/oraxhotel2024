@model DataModels.ProductTable

@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<div class="page-wrapper">

    <!-- Page Content-->
    <div class="page-content-tab">

        <div class="container-fluid">
            <!-- Page-Title -->
            <div class="row">
                <div class="col-sm-12">
                    <div class="page-title-box">
                        <div class="float-end">
                            <ol class="breadcrumb">
                                <li class="breadcrumb-item"><a href="#">الاعدادت</a></li>
                                <li class="breadcrumb-item"><a href="#">الخدمات  </a></li>
                            </ol>
                        </div>
                        <h4 class="page-title">الخدمات الفرعية </h4>
                    </div>
                    <!--end page-title-box-->
                </div>
                <!--end col-->
            </div>
            <div class="row">

                <div class="col-sm-12">
                    <div class="page-title-box">
                        <div class="float-end">

                            <a asp-action="Index">رجوع</a>

                        </div>
                        <h4 class="page-title">حذف خدمة </h4>
                    </div>
                    <!--end page-title-box-->
                </div>



                <center><div id="result"></div></center>

                <dl class="dl-horizontal">
                    <dt>
                        الاسم
                    </dt>
                    <dd>
                        @Html.DisplayFor(model => model.Name)
                    </dd>
                    <dt>
                        الاسم الانجليزي
                    </dt>
                    <dd>
                        @Html.DisplayFor(model => model.NameEn)
                    </dd>

                    <dt>
                        المجموعة الاب
                    </dt>
                    <dd>
                        @Html.DisplayFor(model => model.Fkgroupservicesproductservice.Name)
                    </dd>
                </dl>

                <form asp-action="Delete">
                    <input type="hidden" asp-for="Id" />
                    <input type="submit" value="حذف" class="btn btn-danger" /> |
                   
                </form>

            </div>




            <footer class="footer text-center text-sm-start">
                &copy;
                <script>
                    document.write(new Date().getFullYear())
                </script> شركة اوراكس سوفت <span class="text-muted d-none d-sm-inline-block float-end">
                    للانظمة
                    وتقنية المعلومات
                </span>
            </footer>
        </div>
    </div>
</div>